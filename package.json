{
  "name": "my-app-name",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
     "build:plugin:dev": "cd src/plugins/multi-select && npm install && npm run build",
    "build:plugin:prod": "cd src/plugins/multi-select && npm install && strapi-plugin build",
    "deploy": "strapi deploy",
    "develop": "strapi develop",
    "seed:example": "node ./scripts/seed.js",
    "start": "strapi start",
    "strapi": "strapi",
    "postinstall": "if [ \"$NODE_ENV\" != \"production\" ]; then npm run build:plugin:dev; else npm run build:plugin:prod; fi"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.3.0",
    "@strapi/plugin-users-permissions": "5.3.0",
    "@strapi/strapi": "5.3.0",
    "better-sqlite3": "11.3.0",
    "fs-extra": "^10.0.0",
    "mime-types": "^2.1.27",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node-schedule": "^2.0.0",
    "@types/koa__router": "^8.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}
