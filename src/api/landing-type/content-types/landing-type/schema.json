{
  "kind": "collectionType",
  "collectionName": "landing_types",
  "info": {
    "singularName": "landing-type",
    "pluralName": "landing-types",
    "displayName": "LandingPage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "Template": {
      "type": "component",
      "repeatable": true,
      "component": "shared.template-landing"
    },
    "ab_test_configs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ab-test-config.ab-test-config",
      "inversedBy": "landing_types"
    },
    "Main_Title": {
      "type": "string",
      "required": true
    },
    "Main_SubTitle": {
      "type": "string",
      "required": true
    },
    "Main_Description": {
      "type": "text",
      "required": true
    },
    "Button_Text": {
      "type": "string",
      "required": true
    },
    "Text_In_Border": {
      "type": "text",
      "required": true
    },
    "Video_Section_Title": {
      "type": "string",
      "required": false
    },
    "Video_URL": {
      "type": "string",
      "required": true
    },
    "icons_list": {
      "type": "component",
      "repeatable": false,
      "component": "landing.icons-list",
      "required": true
    },
    "hero_section_media": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "features_list": {
      "displayName": "features_list",
      "type": "component",
      "repeatable": false,
      "component": "landing.features-list",
      "required": true
    },
    "faq_list": {
      "displayName": "faq_list",
      "type": "component",
      "repeatable": false,
      "component": "landing.faq-list",
      "required": true
    }
  }
}
